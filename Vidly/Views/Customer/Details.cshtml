@model Vidly.ViewModels.CustomerViewModel
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<ul>
    <li>MembershipType: @Model.MembershipType.Name </li>
    @{
        if ( @Model.BirthDate != null )
        {
            <li>@Model.BirthDate</li>
        }
    }
    
</ul>

